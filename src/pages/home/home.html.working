<ion-header>
  <ion-navbar>
    <ion-title>
      CapDB
    </ion-title>
  </ion-navbar>
</ion-header>

<ion-content padding>
  <ul *ngIf="results$ | async as results else noData">
      <table style="width:100%" border="1px solid black" >
        <tr><th>Colum Name</th><th>Data Type</th></tr>
        <tr *ngFor="let result of results[2]"><td>{{ result.name }}</td><td>{{ result.type }}</td></tr>
      </table>
  </ul>
  <ng-template #noData>No Data Available</ng-template>
</ion-content>
